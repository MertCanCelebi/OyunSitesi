@model List<KategoriModel>
@{
    ViewData["Title"] = "Kategoriler";
}

<div class="row">
    <div class="col">
        <h1 class="display-4" key=30>@ViewData["Title"]</h1>
        <hr />
    </div>
</div>

<div class="row mb-3">
    <div class="col">
        <table class="table table-hover table-sm">
            <tbody>
                @foreach (KategoriModel kategori in Model)
                {
                    <tr>
                        <a asp-controller="Kategori" asp-action="KategoriOyun" asp-route-id="@kategori.Id"><h4>@kategori.Ad</h4></a>
                    
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>